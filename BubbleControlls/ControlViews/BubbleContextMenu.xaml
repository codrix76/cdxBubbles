<UserControl x:Class="BubbleControlls.ControlViews.BubbleContextMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BubbleControlls.ControlViews"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="MainGrid">
        <Canvas x:Name="MenuCanvas" d:Background="WhiteSmoke">
            <local:BubbleRingControl x:Name="MenuRing" />
        </Canvas>
    </Grid>
</UserControl>
